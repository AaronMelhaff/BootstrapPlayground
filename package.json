{
  "name": "bootstrap-playground",
  "version": "1.0.0",
  "description": "A place to test and edit bootstrap configuration.",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass -w \"src/scss/\" -o \"public/styles/\"",
    "start": "concurrently \"nodemon --inspect index.js\" \"yarn run build-css\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "git+https://github.com/AaronMelhaff/BootstrapPlayground.git",
  "keywords": [
    "bootstrap",
    "sass",
    "test",
    "playground",
    "node"
  ],
  "author": "Aaron Melhaff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AaronMelhaff/BootstrapPlayground/issues"
  },
  "homepage": "https://github.com/AaronMelhaff/BootstrapPlayground#readme",
  "private": false,
  "dependencies": {
    "bootstrap": "^4.2.1",
    "concurrently": "^4.1.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10"
  }
}
